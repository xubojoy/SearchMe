.addcompany-container{
    background-color: #FBF9FE;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
    flex: 1;
    min-height: 100%;
    font-size: 32rpx;
}

.logo-img-bg{
  /*margin-top: 20rpx;*/
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  background-color: purple;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.logo_img{
  color: rebeccapurple;
  width: 200rpx;
  height: 200rpx;
  border-radius: 100px;
  background-color: powderblue;
}

.content-bg{
  padding-top: 20rpx;
  padding-left: 10rpx;
}

.title-text{
  /*background-color: cyan;*/
  float: left;
  word-break: break-all;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  justify-content: space-around;
  line-height: 100rpx;
}

.title-content{
  /*background-color: purple;*/
  height: 100rpx;
}

.code-text{
  float: left;
  word-break: break-all;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  justify-content: space-around;
  line-height: 100rpx;
}
.code-content{
  /*background-color: purple;*/
  height: 100rpx;
}